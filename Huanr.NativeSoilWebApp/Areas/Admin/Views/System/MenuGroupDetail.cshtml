@{
    Huanr.Models.NativeSoil.tSystemMenuGroup tSystemMenuGroup =ViewBag.tSystemMenuGroup;

    ViewData["Title"] = "MenuGroupDetail";
    Layout = "~/Views/Shared/_AdminDialogPageLayout.cshtml";
}
<form class="zero_form zero_form_btns_fixed zero_form_view" id="menuGroupViewForm">
        <dl class="zero_form_group">
            <dt>菜单分组信息</dt>
            <dd><div class="zero_form_text">分组名称：</div><div class="zero_form_input">@tSystemMenuGroup.GroupName</div></dd>
            <dd><div class="zero_form_text">分组备注：</div><div class="zero_form_input">@tSystemMenuGroup.GroupRemark</div></dd>
            <dd><div class="zero_form_text">分组排序：</div><div class="zero_form_input">@tSystemMenuGroup.GroupSort</div></dd>
            <dd><div class="zero_form_text">分组删除：</div><div class="zero_form_input">@if (tSystemMenuGroup.GroupDeleteStatus){<span>是</span>}else {<span>否</span> }</div></dd>
            <dd><div class="zero_form_text">录入时间：</div><div class="zero_form_input">@tSystemMenuGroup.GroupCreateTime.ToString("yyyy年MM月dd日HH时mm分ss秒")</div></dd>
        </dl>

        <div class="zero_form_btns">
            <input type="button" name="zero_btnCancel" value="关闭" onclick="parentDialogObj.close()" />
        </div>
</form>
@section StyleSection{ }
@section JavaScriptSection{}